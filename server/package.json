{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build",
    "start": "node ./dist/index.js",
    "dev": "nodemon ./src/index.ts",
    "build-shared": "tsc -p ./shared/tsconfig.json",
    "watch-shared": "tsc -p ./shared/tsconfig.json --watch",
