    "migration:rollback": "env-cmd npx sequelize-cli db:migrate:undo:all --to",
    "migrate": "env-cmd npx sequelize-cli db:migrate",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ansis": "^3.3.2",
    "axios": "^1.8.4",
